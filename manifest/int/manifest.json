{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0",
  "id": "ab93102c-869b-4d34-a921-a31d3e7f76ef",
  "packageName": "io.ngrok.taskmeow",
  "developer": {
    "name": "Yuri Dogandjiev",
    "websiteUrl": "https://taskmeow.ngrok.io",
    "privacyUrl": "https://taskmeow.ngrok.io/privacypolicy",
    "termsOfUseUrl": "https://taskmeow.ngrok.io/termsofuse"
  },
  "name": {
    "short": "Task Meow (INT)",
    "full": "Task Meow (INT) for Microsoft Teams"
  },
  "description": {
    "short": "Task Meow to do list app.",
    "full": "Task Meow is a simple to do list app that helps you track the important things in your life."
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "accentColor": "#ffffff",
  "configurableTabs": [
    {
      "configurationUrl": "https://taskmeow.ngrok.io/config/?inTeamsMSAL=true",
      "scopes": ["team", "groupchat"]
    }
  ],
  "staticTabs": [
    {
      "contentUrl": "https://taskmeow.ngrok.io/?inTeamsMSAL=true",
      "entityId": "myTasks",
      "name": "My Tasks",
      "scopes": ["personal"]
    },
    {
      "contentUrl": "https://taskmeow.ngrok.io/profile/?inTeamsMSAL=true",
      "entityId": "myProfile",
      "name": "My Profile",
      "scopes": ["personal"]
    }
  ],
  "bots": [
    {
      "botId": "ab93102c-869b-4d34-a921-a31d3e7f76ef",
      "scopes": ["personal", "team", "groupchat"]
    }
  ],
  "permissions": ["messageTeamMembers", "identity"],
  "validDomains": ["taskmeow.ngrok.io"]
}
