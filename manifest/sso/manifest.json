      "scopes": [
        "personal"
      ]
    },
    {
      "contentUrl": "https://taskmeow.com/profile/?inTeamsSSO=true",
      "entityId": "myProfile",
      "name": "My Profile",
      "scopes": [
        "personal"
      ]
    }
